# System-wide .profile for sh(1)

if [ -x /usr/libexec/path_helper ]; then
	eval `/usr/libexec/path_helper -s`
fi

if [ "${BASH-no}" != "no" ]; then
	[ -r /etc/bashrc ] && . /etc/bashrc
fi

export JAVA_HOME=/Library/Java/JavaVirtualMachines/jdk1.8.0_121.jdk/Contents/Home

export CLASSPAHT=.:$JAVA_HOME/lib/dt.jar:$JAVA_HOME/lib/tools.jar

export PATH=$JAVA_HOME/bin:$PATH:

export GOROOT=/programertools/go/go1_10
#export GOBIN=$GOROOT/bin         
export PATH=$PATH:$GOROOT/bin:$GOPATH/bin      
export GOPATH=/project/goworkspace


export PATH=/usr/include/c++/4.2.1:$PATH:$GOBIN


export PATH=/programertools/mongodb/mongodb/bin:$PATH:$GOBIN


export PATH=/Applications/XAMPP/bin:$PATH:$GOBIN

#export PATH=/Library/Frameworks/Python.framework/Versions/3.6/bin:$PATH
